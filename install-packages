# custom installs
mkdir -p ~/Downloads
cd ~/Downloads
wget https://dl.google.com/linux/direct/google-chrome-stable_current_amd64.deb
sudo apt -y install ./*.deb
rm -rf *.deb
mkdir -p ~/.config/suckless
cd ~/.config/suckless
git clone https://github.com/A-Larsen/dwm.git
cd dwm
rm -rf config.h
sudo make && sudo make clean install
cd ~/.config
git clone https://github.com/A-Larsen/nvim.git
cd ~/Downloads
wget https://github.com/neovim/neovim/releases/latest/download/nvim-linux-x86_64.tar.gz
tar -xzf nvim-linux-x86_64.tar.gz
cd nvim-linux-x86_64
sudo cp bin/nvim /usr/local/bin/
sudo cp -r lib/nvim /usr/local/lib/
sudo cp -r share/nvim /usr/local/share/
rm -rf nvim-linux-x86_64 nvim-linux-x86_64.tar.gz
